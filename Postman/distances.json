							"});\r",
							"\r",
							"pm.test(\"Response contains distances array\", function () {\r",
							"    let resp = pm.response.json();\r",
							"    pm.expect(resp).to.have.property(\"distances\");\r",
							"    pm.expect(resp.distances).to.be.an(\"array\");\r",
							"});\r",
							"\r",
							"pm.test(\"Each distance entry is properly structured\", function () {\r",
							"    let resp = pm.response.json();\r",
							"    if (resp.distances.length > 0) {\r",
							"        pm.expect(resp.distances[0]).to.have.all.keys(\"from\", \"to\", \"distance\");\r",
							"    }\r",
